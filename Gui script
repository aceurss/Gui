from PyQt5 import QtWidgets, QtGui, QtCore

class ShrimpGameUI(QtWidgets.QMainWindow): def init(self): super().init() self.setWindowTitle("Moon X - Shrimp Game") self.setGeometry(100, 100, 450, 500) self.setStyleSheet("background-color: #1e1e1e; color: white;")

self.initUI() def initUI(self): central_widget = QtWidgets.QWidget() self.setCentralWidget(central_widget) layout = QtWidgets.QHBoxLayout(central_widget) # Sidebar sidebar = QtWidgets.QVBoxLayout() sidebar.setSpacing(10) sidebar_buttons = ["Main", "Games", "Trolling", "Gun Mods", "Settings"] for btn in sidebar_buttons: button = QtWidgets.QPushButton(btn) button.setStyleSheet("background-color: #2c2c2c; color: white; border: none; padding: 12px; font-size: 12px; text-align: left;") sidebar.addWidget(button) layout.addLayout(sidebar, 1) # Main Content content = QtWidgets.QVBoxLayout() title = QtWidgets.QLabel("Games") title.setFont(QtGui.QFont("Arial", 14, QtGui.QFont.Bold)) title.setStyleSheet("padding-bottom: 10px;") content.addWidget(title) # Completion Time comp_label = QtWidgets.QLabel("Completion Time") comp_label.setStyleSheet("font-size: 11px;") comp_slider = QtWidgets.QSlider(QtCore.Qt.Horizontal) comp_slider.setStyleSheet("background-color: #333;") content.addWidget(comp_label) content.addWidget(comp_slider) # Tug of War tug_label = QtWidgets.QLabel("Tug Of War üè¥") tug_label.setStyleSheet("font-size: 11px;") auto_tug = QtWidgets.QCheckBox("Auto-Tug") auto_tug.setStyleSheet("font-size: 11px;") tug_slider = QtWidgets.QSlider(QtCore.Qt.Horizontal) tug_slider.setStyleSheet("background-color: #333;") tug_int_label = QtWidgets.QLabel("Tug Interval") tug_int_label.setStyleSheet("font-size: 11px;") content.addWidget(tug_label) content.addWidget(auto_tug) content.addWidget(tug_int_label) content.addWidget(tug_slider) # Glass Bridge bridge_label = QtWidgets.QLabel("Glass Bridge üåä") bridge_label.setStyleSheet("font-size: 11px;") no_break_label = QtWidgets.QLabel("No-Break Usage") no_break_label.setStyleSheet("font-size: 11px;") content.addWidget(bridge_label) content.addWidget(no_break_label) # Apply Button apply_button = QtWidgets.QPushButton("Apply Settings") apply_button.setStyleSheet("background-color: #444; color: white; padding: 10px; border-radius: 5px;") content.addWidget(apply_button) layout.addLayout(content, 3) 

if name == "main": app = QtWidgets.QApplication([]) window = ShrimpGameUI() window.show() app.exec_()

